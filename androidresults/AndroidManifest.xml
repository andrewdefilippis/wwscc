<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.wwscc.android.Results"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="6"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.CHANGE_WIFI_MULTICAST_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:label="@string/app_name" >
        <activity
            android:name="org.wwscc.android.Results.SeriesSelector"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.wwscc.android.Results.Browser"
            android:label="@string/title_activity_browser" >
        </activity>
        <activity
            android:name="org.wwscc.android.Results.EventClassSelect"
            android:label="@string/title_activity_event_select" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.wwscc.android.Results.SeriesSelector" />
        </activity>
    </application>

</manifest>